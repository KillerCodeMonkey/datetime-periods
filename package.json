{
  "name": "valid-datetimes",
  "version": "1.0.0",
  "description": "Get valid years down to seconds for a date and optional min and max date",
  "scripts": {
    "test": "jest --coverage",
    "build": "rm -rf ./lib && microbundle --ts -i ./src/index.ts -o ./lib",
    "dev": "rm -rf ./dev-build && microbundle watch --ts -i ./src/index.ts -o ./dev-build"
  },
  "keywords": [
    "dates",
    "datetime",
    "valid",
    "generate"
  ],
  "main": "lib/valid-datetimes.js",
  "umd:main": "lib/valid-datetimes.umd.js",
  "module": "lib/valid-datetimes.mjs",
  "source": "src/index.ts",
  "author": "Bengt Wei√üe",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "jest": "^24.3.1",
    "microbundle": "^0.11.0",
    "ts-jest": "^24.0.0"
  }
}
