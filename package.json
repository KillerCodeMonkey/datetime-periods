{
  "name": "datetime-periods",
  "version": "1.2.0",
  "description": "Generates a valid list of years down to seconds for a date and min and max date",
  "scripts": {
    "test": "jest --coverage && npm run lint",
    "lint": "eslint ./src/index.ts",
    "build": "rm -rf ./lib && microbundle --ts -i ./src/index.ts -o ./lib",
    "dev": "rm -rf ./dev-build && microbundle watch --ts -i ./src/index.ts -o ./dev-build"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KillerCodeMonkey/datetime-periods.git"
  },
  "bugs": {
    "url": "https://github.com/KillerCodeMonkey/datetime-periods/issues"
  },
  "homepage": "https://github.com/KillerCodeMonkey/datetime-periods",
  "keywords": [
    "dates",
    "datetime",
    "valid",
    "generate",
    "periods",
    "javascript",
    "browser",
    "validation",
    "generator",
    "picker",
    "nodejs",
    "node",
    "range"
  ],
  "engines": {
    "node": ">= 18"
  },
  "main": "lib/datetime-periods.js",
  "umd:main": "lib/datetime-periods.umd.js",
  "module": "lib/datetime-periods.mjs",
  "source": "src/index.ts",
  "types": "lib/src/index.d.ts",
  "author": "Bengt Wei√üe",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "acorn": "^8.14.0",
    "eslint": "^8.45.0",
    "jest": "^29.7.0",
    "microbundle": "^0.15.1",
    "ts-jest": "^29.2.6",
    "typescript": "^5.7.3"
  }
}
